# basic switch-case statement, demonstrating the falling-through behaviour.

# Output:
# 30
# 40
# 3
# 50

# Program:

begin
    int x = 3;
    int y = 0;
    switch x
    do 
        case 1:
            y = 10;
            println y
        case 2:
            y = 20;
            println y
        case 3:
            y = 30;
            println y
        case 4:
            y = 40;
            println y
        default:
            y = 50
    done;

    println x;
    println y
end
