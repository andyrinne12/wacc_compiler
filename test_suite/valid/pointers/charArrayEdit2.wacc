# Create an array of characters and use a pointer computation to change its elements

# Output:
# "success"

# Program:

begin
  char[] word = ['s','z','c','x','e','s','t'];

  int* p1 = word + 4 + 1;
  *p1 = 'u';
  p1 = word + 4 + 3;
  *p1 = 'c';
  p1 = word + 4 + 6;
  *p1 = 's';

  string* w = word;

  println w
end